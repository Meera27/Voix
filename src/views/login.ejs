    <div class="container main">
      <div class="row">
        <div class="col-md-3">
        </div>
        <div class="col-md-6">
          <div class="panel ">
            <div class="panel-body main-frame">
              <div
                style="box-shadow: 0 4px 8px 0 rgba(103,14,40,0.2), 0 6px 20px 0 rgba(103,14,40,0.19); height: auto;width:auto;color: white !important;">

                <div class="to-right p-3" style="margin-top:120px">
                  <form id="newform" action="/login/valid" method="POST">
                    <div class="form-row">
                      <div class="col-lg-6 mx-auto pt-5">
                        <input type="text" class="fnames" size="30"  placeholder="Email" id="exampleInputEmail1" name="email"
                          required>
                      </div>
                    </div>
                    <div class="form-row">
                      <div class="col-lg-6 mx-auto pt-5">
                        <input type="password" class="fnames" size="30" placeholder="Password" id="exampleInputPassword1"
                          name="password" required>
                      </div><br><br>
                    </div>
                    <div class="col-lg-7 mx-auto" style="text-align: center;">
                      <label for="error" style="color: red; text-align: center;">
                        <%= (typeof error !="undefined" ? error : "" ) %>
                      </label>
                    </div>
                    <div class="form-row">
                      <div class="col-lg-6 mx-auto" style="text-align:center;">
                        <button type="submit" class="btn btn-outline-dark btn-lg mt-3 mb-5 lbtn"
                          id="submitme">Login</button>


                          <!-- <div class="g-signin2" data-onsuccess="onSignIn"></div> -->


                      </div>
                    </div>
                    <div class="col-lg-7 mx-auto " style="text-align: center;">
                      <p style="color:#5BB8FE; text-align: center;">New User?  <a href="/signup">Sign Up</a></p>                         
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
